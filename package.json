{
  "name": "rnTemplateApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint './src/**/*.{ts,tsx}' && tsc",
    "lint:fix": "eslint './src/**/*.{ts,tsx}' --fix",
    "tsc": "tsc",
    "ios": "react-native run-ios --configuration devDebug",
    "ios:devRelease": "react-native run-ios --configuration devRelease",
    "ios:stgDebug": "react-native run-ios --configuration stgDebug",
    "ios:stgRelease": "react-native run-ios --configuration stgRelease",
    "ios:prdRelease": "react-native run-ios --configuration Release",
    "ios:prdDebug": "react-native run-ios --configuration Debug",
    "android": "react-native run-android --variant=devDebug --appIdSuffix dev.debug",
    "android:devRelease": "react-native run-android --variant=devRelease --appIdSuffix dev",
    "android:stgDebug": "react-native run-android --variant=stgDebug --appIdSuffix stg.debug",
    "android:stgRelease": "react-native run-android --variant=stgRelease --appIdSuffix stg",
    "android:prdRelease": "react-native run-android --variant=prdRelease",
    "android:prdDebug": "react-native run-android --variant=prdDebug --appIdSuffix debug",
    "android:clean": "cd android && ./gradlew clean",
    "ios:clean": "cd ios && rm -rf Podfile.lock && rm -rf Pods",
    "rename": "react-native-rename"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.6",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "@reduxjs/toolkit": "^1.8.1",
    "axios": "^0.27.2",
    "dayjs": "^1.11.6",
    "i18next": "^21.8.9",
    "lottie-colorify": "^0.8.0",
    "lottie-ios": "3.2.3",
    "lottie-react-native": "5.1.2",
    "react": "18.1.0",
    "react-i18next": "^11.17.1",
    "react-native": "0.70.3",
    "react-native-config": "^1.4.11",
    "react-native-device-info": "^10.3.0",
    "react-native-fast-image": "^8.6.1",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal": "^13.0.1",
    "react-native-permissions": "^3.6.1",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-safe-area-view": "^1.1.1",
    "react-native-screens": "^3.13.1",
    "react-native-secure-key-store": "^2.0.9",
    "react-native-shadow-2": "^6.0.5",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^12.3.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.0.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.67.3",
    "@types/react-native-vector-icons": "^6.4.12",
    "@types/react-test-renderer": "^17.0.1",
    "@types/validator": "^13.7.3",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^6.0.1",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.72.3",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
